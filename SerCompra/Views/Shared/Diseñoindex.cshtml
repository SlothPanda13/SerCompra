        <a href="#v-pills-home" class="navbar-brand">
            <img src="~/Imagenes/logo1.png" style="width:50%;"/>
        </a>

        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#main-menu" aria-controls="main-menu">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" style="justify-content: right" id="main-menu">
            <div>
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link active" aria-current="page" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" href="#">Nosotros</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link active" asp-area="" asp-controller="" asp-action="">Iniciar sesión</a>
                    </li>
                    <li class="nav-item">
                        <button type="button" class="btn btn-primary" data-toggle="modal" data-target="#Modalcontacto">
                            <i class="far fa-envelope"></i> Contáctanos
                        </button>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="https://www.comfacundi.com.co/"><i class="fas fa-power-off"></i> Salir</a>
                    </li>

                </ul>
            </div>

        </div>

    </nav>
    <!-- modal de contactos -->
    <div class="modal fade" id="Modalcontacto" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLabel">Contactenos</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="container">
                        <div class="row">
                            <div class="col">
                                <div class="flex-direc">
                                    <div class="icon-ubica">
                                        <i class="fas fa-phone-alt"></i>
                                    </div>
                                    <h3 class="titulo-form">Telefono</h3>
                                    <p>(+57) 323 900 00 00</p>
                                </div>
                                <div class="flex-direc">
                                    <div class="icon-ubica">
                                        <i class="fas fa-envelope-open-text"></i>
                                    </div>
                                    <h3 class="titulo-form">Correo</h3>
                                    <p>contacto@sercompra.com.co</p>
                                </div>
                                <div class="google-maps">
                                <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d63627.65940912732!2d-74.09267068504955!3d4.642406700000024!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x8e3f9a39b65e4b45%3A0xb245795852177049!2sServicio%20Nacional%20de%20Aprendizaje%20SENA!5e0!3m2!1ses-419!2sco!4v1600893550998!5m2!1ses-419!2sco"
                                        width="430" height="250" style="border:0;" allowfullscreen="" aria-hidden="false"
                                        tabindex="0"></iframe>
                                </div>
                                <form>
                                    <div class="form-row">
                                        <div class="col-6">
                                            <label for="inputAddress">Nombre </label>
                                            <input type="text" class="form-control" placeholder="nombre">
                                        </div>
                                        <div class="col-6">
                                            <label for="inputAddress">Apellido </label>
                                            <input type="text" class="form-control" placeholder="apellido">
                                        </div>
                                    </div>
                                    <div class="form-row">
                                        <div class="col-12">
                                            <label for="inputAddress">Correo </label>
                                            <input type="text" class="form-control" placeholder="Correo">
                                        </div>

                                        <div class="form-group">
                                            <label for="exampleFormControlFile1"></label>
                                            <input type="file" class="form-control-file" id="exampleFormControlFile1">
                                        </div>

                                    </div>
                                    <div class="form-group">
                                        <label for="exampleFormControlTextarea1">Dejanos Aquí tu Opinion </label>
                                        <textarea class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    <button type="button" class="btn btn-primary">Enviar</button>
                </div>
            </div>
        </div>
    </div>
</header>

<!--interior contenido-->
<main role="main">
    @RenderBody()
</main>

